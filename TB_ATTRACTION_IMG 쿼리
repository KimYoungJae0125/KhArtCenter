//볼거리 쿼리

DROP TABLE TB_ATTRACTION_IMG;

DROP SEQUENCE ATTRACTION_IMG_SEQ;


ALTER TABLE TB_ATTRACTION_IMG
DROP CONSTRAINT fk_ATTRACTION_NO;


 CREATE TABLE TB_ATTRACTION_IMG (
    ATTRACTION_IMG_NO   NUMBER,
    ATTRACTION_NO       NUMBER,
    ATTRACTION_PHOTO    CLOB
);

ALTER TABLE TB_ATTRACTION_IMG
ADD CONSTRAINT fk_ATTRACTION_NO FOREIGN KEY (ATTRACTION_NO) REFERENCES TB_ATTRACTION(ATTRACTION_NO) ON DELETE CASCADE;

