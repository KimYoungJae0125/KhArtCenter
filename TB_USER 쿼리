DROP TABLE TB_MEMBER;
CREATE TABLE TB_MEMBER (
    MEMBER_ID   VARCHAR2(2000 BYTE)
    ,MEMBER_PW   VARCHAR2(2000 BYTE)
    ,MEMBER_NAME   VARCHAR2(30 BYTE)
    ,BIRTH   DATE
    ,GENDER   VARCHAR2(10 BYTE)
    ,MEMBER_ADDR   VARCHAR2(2000 BYTE)
    ,NICK   VARCHAR2(2000 BYTE)
    ,MEMBER_GRADE   VARCHAR2(10 BYTE) default '실버'
    ,POINT   NUMBER
    ,email varchar2(2000)
    ,MEMBER_SPOT varchar(30) default '회원'
);
